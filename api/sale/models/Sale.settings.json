{
  "connection": "default",
  "collectionName": "sale",
  "info": {
    "name": "sale",
    "description": "Ticket being on sale"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "ticket": {
      "model": "ticket",
      "via": "sale"
    },
    "end_block": {
      "default": "",
      "type": "integer",
      "required": true
    },
    "begin_block": {
      "default": "",
      "type": "integer",
      "required": true
    },
    "status": {
      "default": "",
      "type": "enumeration",
      "enum": [
        "open",
        "closed",
        "ended",
        "bought"
      ],
      "required": true
    },
    "event": {
      "model": "event",
      "via": "sales"
    },
    "marketer": {
      "model": "marketer",
      "via": "sales"
    },
    "price": {
      "default": "",
      "type": "json"
    },
    "update_height": {
      "default": "",
      "type": "integer"
    }
  }
}