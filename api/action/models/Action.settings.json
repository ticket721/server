{
  "connection": "default",
  "collectionName": "action",
  "info": {
    "name": "action",
    "description": "An action is and EVM Event"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "by": {
      "model": "address",
      "via": "actions_by"
    },
    "to": {
      "model": "address",
      "via": "actions_to"
    },
    "on_ticket": {
      "model": "ticket",
      "via": "actions"
    },
    "action_type": {
      "default": "",
      "type": "enumeration",
      "required": true,
      "enum": [
        "mint",
        "sale",
        "sale_close",
        "buy",
        "transfer",
        "approval",
        "approval_for_all",
        "event",
        "new_admin"
      ]
    },
    "infos": {
      "default": "",
      "type": "json",
      "required": true
    },
    "block": {
      "default": "",
      "type": "integer",
      "required": true
    }
  }
}