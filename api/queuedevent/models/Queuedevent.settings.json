{
  "connection": "default",
  "collectionName": "queuedevents",
  "info": {
    "name": "queuedevent",
    "description": "Informations about event that is pending in a transaction"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "name": {
      "default": "",
      "type": "string",
      "required": true
    },
    "description": {
      "default": "",
      "type": "text",
      "required": true
    },
    "owner": {
      "model": "address",
      "via": "queuedevents"
    },
    "transaction_hash": {
      "default": "",
      "type": "string"
    },
    "address": {
      "default": "",
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}