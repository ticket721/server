{
    "name": "@ticket721/server",
    "private": true,
    "version": "1.0.0",
    "description": "t721-server - Server to store contract data and events for ticket72",
    "main": "./server.js",
    "scripts": {
        "setup": "cd admin && npm run setup",
        "start": "node server.js",
        "strapi": "node_modules/strapi/bin/strapi.js",
        "lint": "node_modules/.bin/eslint api/**/*.js config/**/*.js plugins/**/*.js",
        "postinstall": "node node_modules/strapi/lib/utils/post-install.js && link-module-alias"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ticket721/server.git"
    },
    "strapi": {
        "packageManager": "npm",
        "uuid": "7ced4146-c2c9-459b-81cd-8ca89733b765"
    },
    "engines": {
        "node": ">= 10.0.0",
        "npm": ">= 6.0.0"
    },
    "author": "Iulian Rotaru",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ticket721/server/issues"
    },
    "homepage": "https://github.com/ticket721/server#readme",
    "dependencies": {
        "@htmlacademy/editorconfig-cli": "^1.0.0",
        "@types/node": "^11.9.4",
        "dockerode": "^2.5.8",
        "gulp-typescript": "^5.0.0",
        "husky": "^1.3.1",
        "knex": "latest",
        "link-module-alias": "^1.1.6",
        "linted": "^1.0.5",
        "lodash": "^4.17.5",
        "pg": "7.8.0",
        "portalize": "^1.0.3",
        "rimraf": "^2.6.3",
        "signale": "^1.3.0",
        "strapi": "3.0.0-alpha.24.1",
        "strapi-hook-bookshelf": "3.0.0-alpha.24.1",
        "strapi-hook-knex": "3.0.0-alpha.24.1",
        "tslint": "^5.12.1",
        "typescript": "^3.3.3333"
    },
    "_moduleAliases": {
        "@portal": "./portal"
    },
    "devDependencies": {
        "merge": ">=1.2.1",
        "deep-extend": ">=0.5.1",
        "babel-eslint": "^7.1.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^13.0.0",
        "eslint-plugin-import": "^2.11.0",
        "eslint-plugin-react": "^7.7.0"
    },
    "editorconfig-cli": [
        "./*.json",
        "./*.js",
        "./config/*.json",
        "./hooks_sources/**/*.ts",
        "./tasks/**/*.js"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "editorconfig-cli -x node_modules && tslint --project ."
        }
    }
}
